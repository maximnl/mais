  // ---------- Validate rows_skip
  function do_ajax_file_validate_rows_skip()
  {
    var nomeCampo_rows_skip = "rows_skip";
    var var_rows_skip = scAjaxGetFieldText(nomeCampo_rows_skip);
    var var_script_case_init = document.F1.script_case_init.value;
    x_ajax_file_validate_rows_skip(var_rows_skip, var_script_case_init, do_ajax_file_validate_rows_skip_cb);
  } // do_ajax_file_validate_rows_skip

  function do_ajax_file_validate_rows_skip_cb(sResp)
  {
    oResp = scAjaxResponse(sResp);
    scAjaxRedir();
    sFieldValid = "rows_skip";
    scEventControl_onBlur(sFieldValid);
    scAjaxUpdateFieldErrors(sFieldValid, "valid");
    sFieldErrors = scAjaxListFieldErrors(sFieldValid, false);
    if ("" == sFieldErrors)
    {
      var sImgStatus = sc_img_status_ok;
      scAjaxHideErrorDisplay(sFieldValid);
    }
    else
    {
